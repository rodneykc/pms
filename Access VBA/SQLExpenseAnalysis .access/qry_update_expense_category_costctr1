UPDATE ([tmptbl_expense_re-categorize] INNER JOIN tbl_expense_category ON [tmptbl_expense_re-categorize].CategoryNew = tbl_expense_category.Title) INNER JOIN tbl_expenses ON [tmptbl_expense_re-categorize].ID = tbl_expenses.ID SET tbl_expenses.Expense_Cat = [tbl_expense_category].[ID], tbl_expenses.CostCenter = [tmptbl_expense_re-categorize].[CostCtr], tbl_expenses.[Desc] = IIf([tmptbl_expense_re-categorize].[Desc] Is Not Null And [tmptbl_expense_re-categorize].[Desc]<>'',[tmptbl_expense_re-categorize].[Desc],[tbl_expenses].[Desc])
WHERE (((tbl_expenses.Expense_Cat)<>[tbl_expense_category].[ID])) OR (((tbl_expenses.CostCenter)<>[tmptbl_expense_re-categorize].[CostCtr])) OR (((tbl_expenses.Desc)<>[tmptbl_expense_re-categorize].[Desc]) AND (([tmptbl_expense_re-categorize].Desc) Is Not Null And ([tmptbl_expense_re-categorize].Desc)<>''));
